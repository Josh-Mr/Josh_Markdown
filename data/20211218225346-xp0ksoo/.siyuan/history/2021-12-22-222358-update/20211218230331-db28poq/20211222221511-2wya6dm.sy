					"Type": "NodeCodeBlockCode",
					"Data": "--创建用户\nCREATE USER ps_nc63 IDENTIFIED BY \"ps_nc63\" DEFAULT  TABLESPACE OTHER_DATA01 TEMPORARY TABLESPACE TEMP;\n\n--给与连接\n\nGRANT CONNECT TO ps_nc63 ; \n\nGRANT CREATE SESSION TO ps_nc63;\n\ngrant connect,resource,dba to  warrant_nc63;--dba权限\n\n#### 分配表权限\n\ngrant select on ps_sm_trade to ps_nc63;\n\ngrant select on ps_sm_trade_cus   to ps_nc63;\n\ngrant select on ps_sm_trade_pro   to ps_nc63;\n\ngrant select on ps_sm_trade_sellor to ps_nc63;\n\ngrant select on ps_so_subsc   to ps_nc63;\n\ngrant select on ps_so_subsc_cus  to ps_nc63;\n\ngrant select on ps_so_sign     to ps_nc63;\n\ngrant select on ps_so_sign_cus to ps_nc63;\n\n#### 别名\n\nCREATE PUBLIC SYNONYM  ps_sm_trade for  nc63test.ps_sm_trade;\n\nCREATE PUBLIC SYNONYM  ps_sm_trade_cus   for  nc63test.ps_sm_trade_cus;\n\nCREATE PUBLIC SYNONYM  ps_sm_trade_pro   for  nc63test.ps_sm_trade_pro;\n\nCREATE PUBLIC SYNONYM  ps_sm_trade_sellor for  nc63test.ps_sm_trade_sellor;\n\nCREATE PUBLIC SYNONYM  ps_so_subsc   for      nc63test.ps_so_subsc;\n\nCREATE PUBLIC SYNONYM  ps_so_subsc_cus  for   nc63test.ps_so_subsc_cus;\n\nCREATE PUBLIC SYNONYM  ps_so_sign     for    nc63test.ps_so_sign;\n\nCREATE PUBLIC SYNONYM  ps_so_sign_cus for  nc63test.ps_so_sign_cus;\n\n"
				},
